<div class="jumbotron">
  <div class="container">
    
    <h2>Наши партнеры</h2>

    <div class="row">
      <% Partner.all.each do |partner| %>
        <div class="col-lg-3 col-md-4 col-xs-6 thumb">
            <a href="<%=partner.website%>">
                <%=image_tag partner.picture, class: 'img-fluid', target: "_blank" %>
            </a>
            <p><%= partner.description %></p> 
        </div>
      <% end %>
    </div>
   
  </div>
</div>